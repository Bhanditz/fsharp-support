//  (fSharpTypeMemberDeclaration<TYPE_MEMBER, Members>)* fakeSeparator
;

interface fSharpUnionCaseDeclaration
:
//  fSharpSingletonUnionCaseDeclaration |
  fSharpTypedUnionCaseDeclaration
;

