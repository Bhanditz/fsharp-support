interfaceInherit : longIdentifier<LONG_IDENTIFIER, LongIdentifier> FAKE;
typeInherit : longIdentifier<LONG_IDENTIFIER, LongIdentifier> FAKE;
interfaceImplementation : longIdentifier<LONG_IDENTIFIER, LongIdentifier> FAKE;
open : longIdentifier<LONG_IDENTIFIER, LongIdentifier> FAKE;
do : FAKE;

abstractSlot : FAKE;
otherTypeMember : FAKE;
otherModuleMember : FAKE;



// Identifiers

fSharpIdentifier : IDENTIFIER;

longIdentifier: IDENTIFIER<IDENTIFIER, Identifiers>*;



// Access modifiers

accessModifiers: modifier{ACCESS_MODIFIER, Modifiers}*;

private modifier{ROLE,Getter}
:
  PUBLIC<@ROLE,@Getter> |
  PRIVATE<@ROLE,@Getter> |
  INTERNAL<@ROLE,@Getter>
;



