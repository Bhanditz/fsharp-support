<idea-plugin>
  <name>F# Support</name>
  <id>com.jetbrains.rider.fsharp</id>
  <vendor url="https://jetbrains.com">JetBrains</vendor>
  <description>F# language support in Rider</description>
  <change-notes/>
  <depends>com.intellij.modules.rider</depends>
  <extensions defaultExtensionNs="com.intellij">
    <fileTypeFactory id="F#" implementation="com.jetbrains.resharper.ideaInterop.fileTypes.fsharp.FSharpFileTypeFactory"/>
    <lang.parserDefinition language="F#" implementationClass="com.jetbrains.resharper.ideaInterop.fileTypes.fsharp.FSharpParserDefinition"/>
    <lang.ast.factory language="F#" implementationClass="com.jetbrains.resharper.ideaInterop.fileTypes.fsharp.FSharpAstFactory"/>
    <backend.typedHandler language="F#" implementationClass="com.jetbrains.resharper.editorActions.RiderTypedHandler"/>
    <completion.contributor language="F#" implementationClass="com.jetbrains.resharper.completion.ReSharperCompletionContributor" />
    <backend.markup.adapterFactory language="F#" implementationClass="com.jetbrains.resharper.daemon.RiderMarkupAdapterFactory" />
    <backend.actions.support language="F#" implementationClass="com.jetbrains.resharper.actions.RiderActionSupportPolicy" />
  </extensions>
</idea-plugin>